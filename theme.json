{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "1170px",
            "wideSize": "1170px"
        },
        "spacing": {
            "padding": true,
            "margin": true,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "typography": {
            "dropCap": false,
            "fontFamilies": [
                {
                    "fontFamily": "'Assistant', sans-serif",
                    "name": "Assistant",
                    "slug": "assistant",
                    "fontFace": [
                        {
                            "fontFamily": "Assistant",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/assistant/Assistant-Light.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Assistant",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/assistant/Assistant-Regular.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Assistant",
                            "fontWeight": "500",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/assistant/Assistant-Medium.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Assistant",
                            "fontWeight": "600",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/assistant/Assistant-SemiBold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Assistant",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/assistant/Assistant-Bold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Assistant",
                            "fontWeight": "800",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/assistant/Assistant-ExtraBold.ttf"
                            ]
                        }
                    ]
                }
            ]
        }
    },
    "styles": {
        "elements": {
            "button": {
                "border": {
                    "radius": "5px"
                }
            }
        },
        "blocks": {
            "core/heading": {
                "spacing": {
                    "margin": {
                        "top": "var( --wp--preset--spacing--0 )",
                        "bottom": "var( --wp--preset--spacing--0 )"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "name": "default-header",
            "title": "Defatulf Header",
            "area": "header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ],
    "customTemplates": [
        {
            "name": "front-page",
            "title": "Home"
        }
    ]
}